{
  "name": "khaled/breakdown-hotline",
  "authors": [
    {
      "name": "kaldev-a",
      "email": "kaled.alazem@gmail.com"
    }
  ],
  "require-dev": {
    "squizlabs/php_codesniffer": "3.*",
    "automattic/vipwpcs": "^2.3",
    "phpunit/phpunit": "^9.5",
    "php-mock/php-mock": "^2.3",
    "php-parallel-lint/php-parallel-lint": "^1.3",
    "yoast/phpunit-polyfills": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "BreakdownHotline\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "BreakdownHotline\\Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "require": {
    "ext-gd": "*"
  }
}
